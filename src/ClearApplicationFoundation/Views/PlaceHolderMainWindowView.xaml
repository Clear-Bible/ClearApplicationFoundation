<Page
    x:Class="ClearApplicationFoundation.Views.PlaceHolderMainWindowView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ClearApplicationFoundation.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:ClearApplicationFoundation.ViewModels"
    MinWidth="800"
    MinHeight="450"
    d:DataContext="{d:DesignInstance Type=viewModels:PlaceHolderMainWindowViewModel,
                                     IsDesignTimeCreatable=True}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Label Content="{Binding Title}" FontSize="40" />
    </Grid>
</Page>
