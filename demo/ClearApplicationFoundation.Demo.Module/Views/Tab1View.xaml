<UserControl x:Class="ClearApplicationFoundation.Demo.Module.Views.Tab1View"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ClearApplicationFoundation.Demo.Module.Views"
             xmlns:viewModels="clr-namespace:ClearApplicationFoundation.Demo.Module.ViewModels"
             d:DataContext="{d:DesignInstance Type=viewModels:Tab1ViewModel, IsDesignTimeCreatable=True}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Background="Crimson">
    <Grid>
            <Label Content="{Binding DisplayName}" FontSize="20"></Label>
    </Grid>
</UserControl>
